{
  "name": "hcms-partners",
  "version": "0.1.0",
  "description": "HCMS Partners",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=22.x"
  },
  "browserslist": {
    "production": [
      "firefox >=103",
      "chrome >=88",
      "safari >=14.1",
      "ios_saf >=14.5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{html,json,yaml}": "prettier --write --list-different",
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "deploy": "firebase deploy",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.26.3",
    "@rollup/plugin-swc": "^0.4.0",
    "@swc/core": "^1.10.1",
    "@types/node": "^22.10.2",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-solid": "^0.14.5",
    "husky": "^9.1.7",
    "lightningcss": "^1.28.2",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rollup-plugin-minify-template-literals": "^1.1.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass-embedded": "^1.83.0",
    "stylelint": "^16.11.0",
    "stylelint-config-hudochenkov": "^11.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-no-unsupported-browser-features": "^8.0.2",
    "stylelint-order": "^6.0.4",
    "terser": "5.31.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "unplugin-inject-preload": "^3.0.0",
    "vite": "^6.0.3",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-optimize-css-modules": "^1.2.0",
    "vite-plugin-sass-dts": "^1.3.29",
    "vite-plugin-solid": "^2.11.0",
    "vite-plugin-svgo": "^1.6.2"
  },
  "dependencies": {
    "@solidjs/router": "0.10.1",
    "solid-js": "1.8.5"
  },
  "resolutions": {
    "@babel/helper-module-imports": "^7.25.9",
    "known-css-properties": "^0.34.0",
    "mdn-data": "^2.13.0",
    "svgo": "^3.3.2"
  },
  "packageManager": "yarn@3.8.7"
}
