{
  "name": "hcms-partners",
  "version": "0.1.0",
  "description": "HCMS Partners",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=20.x"
  },
  "browserslist": {
    "production": [
      "firefox >=103",
      "chrome >=88",
      "safari >=14.1",
      "ios_saf >=14.5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "deploy": "firebase deploy",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.24.5",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-regexp": "^2.5.0",
    "eslint-plugin-solid": "^0.14.0",
    "husky": "^9.0.11",
    "lightningcss": "^1.24.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup-plugin-minify-template-literals": "^1.1.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.0",
    "stylelint": "^16.5.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-order": "^6.0.4",
    "terser": "5.31.0",
    "typescript": "^5.4.5",
    "unplugin-inject-preload": "^2.0.3",
    "vite": "^5.2.11",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-optimize-css-modules": "^1.0.6",
    "vite-plugin-sass-dts": "^1.3.18",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-svgo": "^1.4.0"
  },
  "dependencies": {
    "@solidjs/router": "0.10.1",
    "solid-js": "1.8.5"
  },
  "resolutions": {
    "svgo": "^3.2.0"
  },
  "packageManager": "yarn@3.8.2"
}
